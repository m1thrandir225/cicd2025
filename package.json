{
  "name": "Pollz",
  "version": "1.0.0",
  "description": "Easy and fast polls with live statistics",
  "scripts": {
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "backend:run": "cd backend && go run main.go",
    "backend:build": "cd backend && go build -o ../bin/server main.go",
    "start": "concurrently \"npm run backend:run\" \"npm run frontend:dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "author": "Sebastijan Zindl",
  "license": "ISC",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
